<script>
    $(document).ready(function () {
        $(document).on('click', '#my_form #submit', function (e) {
            e.preventDefault();
            var id = $("#my_form #idorder").val();
            if (id === "") {
                $("#my_form #idorder").focus();
                Swal.fire('لا يمكن ترك الحقل فارغ')
            } else {
                $.ajax({
                    url: '/Trace-Order',
                    type: 'POST',
                    data: {
                        id: id
                    },
                    success: function (response) {
                        Swal.fire(response.data[0].status)
                    },
                    error: function (err) {
                        Swal.fire("حدث خطأ قد يكون رقم المعرف خطأ")
                    },
                });
                /**Ajax code ends**/
            }
        });
    });
</script>
